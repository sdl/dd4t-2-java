<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="SiteEditSettings" class="org.dd4t.springmvc.siteedit.SiteEditSettings">
        <!-- set this to true to enable SiteEdit across this application -->
        <property name="enabled">
            <value>false</value>
        </property>

        <!-- location of the javascript file -->
        <property name="contentmanager">
            <value>http://hostname/WebUI/Editors/SiteEdit/Views/Bootstrap/Bootstrap.aspx?mode=js</value>
        </property>
    </bean>
</beans>